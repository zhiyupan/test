    <!-- http://purl.org/net/p-plan#correspondsToStep -->


    <owl:ObjectProperty rdf:about="http://purl.org/net/p-plan#correspondsToStep">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
